{
    "version": 2,
    "env": {
        "DATABASE_URL": "@database_url",
        "JWT_SECRET": "@jwt_secret",
        "JWT_TOKEN_EXPIRY": "@jwt_token_expiry"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ], 
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}